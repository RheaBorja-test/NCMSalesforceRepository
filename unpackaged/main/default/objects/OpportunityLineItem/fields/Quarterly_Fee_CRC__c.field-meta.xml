<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quarterly_Fee_CRC__c</fullName>
    <formula>IF(BEGINS(PricebookEntry.Product2.Name, &quot;QUARTERLY - FEES&quot;), (1*TotalPrice), 
IF(CONTAINS(PricebookEntry.Product2.Name, &quot;QUARTERLY PARTIAL&quot;), (1*TotalPrice),
NULL))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Quarterly Fee - CRC</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
