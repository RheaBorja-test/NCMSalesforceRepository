<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>State_must_be_3_Characters</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(State__c) &gt; 3</errorConditionFormula>
    <errorDisplayField>State__c</errorDisplayField>
    <errorMessage>State must only be three character abbreviation</errorMessage>
</ValidationRule>
