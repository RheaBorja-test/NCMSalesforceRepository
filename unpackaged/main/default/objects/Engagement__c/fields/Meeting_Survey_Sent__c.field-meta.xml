<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Meeting_Survey_Sent__c</fullName>
    <description>Returns TRUE if either the Survey Send Time OR the Meeting CSI Sent fields are before today.</description>
    <formula>OR(
 Survey_Send_Time__c &lt; NOW(),
 Meeting_CSI_Sent__c &lt; TODAY()
)</formula>
    <label>Meeting Survey Sent</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
